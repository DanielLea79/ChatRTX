{
    "name": "trt-llm-rag-windows-client",
    "version": "0.0.1",
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "electron": "electron build/electron/main.js",
        "cp-engine": "npx rimraf build/bridge_commands && copyfiles -u 1 src/bridge_commands/** build",
        "electron-ts": "tsc -p src/electron/tsconfig.json --outDir build && npm run cp-engine",
        "start-electron": "npm run electron-ts && npm run electron",
        "start-electron-mock": "npm run electron-ts && cross-env MOCK=true npm run electron",
        "watch": "cross-env NODE_ENV=dev rollup --bundleConfigAsCjs -c rollup.config.js --watch",
        "build-assets": "npx rimraf build && cross-env NODE_ENV=production rollup --bundleConfigAsCjs -c rollup.config.js",
        "add-pythonia": "copyfiles -u 1 node_modules/pythonia/** dist/win-unpacked/resources/app.asar.unpacked/node_modules",
        "add-caller": "copyfiles -u 1 node_modules/caller/** dist/win-unpacked/resources/app.asar.unpacked/node_modules",
        "add-missing-node-modules": "npm run add-pythonia && npm run add-caller",
        "package": "electron-builder --config electron-builder.json -c.extraMetadata.main=build/electron/main.js --win && npm run add-missing-node-modules",
        "build-electron": "npx rimraf dist && npm run build-assets && npm run electron-ts && npm run package",
        "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@mui/material": "^5.15.16",
        "electron-log": "^5.1.5",
        "i18next": "^23.11.4",
        "mui-markdown": "^1.1.13",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-i18next": "^14.1.1",
        "rxjs": "^7.8.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.24.1",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-image": "^3.0.3",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/dom-speech-recognition": "^0.0.4",
        "@types/react": "^18.3.1",
        "@types/react-dom": "^18.3.0",
        "autoprefixer": "^10.4.19",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "electron": "^30.0.2",
        "electron-builder": "^24.13.3",
        "prettier": "^3.3.2",
        "pythonia": "^1.1.3",
        "rimraf": "^5.0.5",
        "rollup": "^4.17.2",
        "rollup-plugin-node-builtins": "^2.0.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "sass": "^1.76.0",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5"
    }
}
